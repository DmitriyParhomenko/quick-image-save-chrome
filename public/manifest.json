{
    "manifest_version": 3,
    "name": "Quick Image Save",
    "version": "1.0.0",
    "description": "Saving all selected images to the local computer from the page.",
    "author": {
        "email": "dmitriyparhomenko140@gmail.com"
    },
    "homepage_url": "https://devraiks.com",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting"
    ],
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}